package com.obj.model.vo;

public class InitblockTest {
	//초기화 블록 테스트하기
	public String test;
	public int testnum;
	
	//초기화 블록 선언하기
	{
		System.out.println("InitblockTest 클래스 생성!!");
		test="야구화이팅!";
		testnum=200;
		//조건문, 반복문 사용할 수 있음.
		int count=0;
		for(int i=1;i<=10;i++) {
			count+=i;
		}
		testnum+=count;	
		
	}

}
